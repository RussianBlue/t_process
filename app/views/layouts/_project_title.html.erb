<div class="space-8"></div>
<div class="page-header">
  <h1>
    <%= current_project.title %>
    <small>
      <i class="icon-double-angle-right"></i>
      PROJECTS
    </small>
  </h1>
</div><!-- /.page-header -->

<div class="curriculum-header">
