<%= simple_form_for(@progress) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_id %>
    <%= f.input :curriculum_id %>
    <%= f.input :lesson %>
    <%= f.input :process %>
    <%= f.input :progress_type_id %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
