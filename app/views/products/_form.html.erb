<%= simple_form_for(@product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :curriculum_id %>
    <%= f.input :lesson %>
    <%= f.input :content_use %>
    <%= f.input :p_data %>
    <%= f.input :s_data %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
